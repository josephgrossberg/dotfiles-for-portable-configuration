[core]
  bare = false
  editor = emacs -q -l ~/git-init-file.el
  filemode = true
  ignorecase = false
  logallrefupdates = true
  pager = cat
  repositoryformatversion = 0
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  ci = commit
  commend = commit --amend --no-edit
  co = checkout
  di = diff
  difflog = log -p
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  st = status

[user]
  name = Joe Grossberg
  email = josephgrossberg+github@gmail.com
[apply]
  whitespace = nowarn
[push]
  default = current
[pull]
  rebase = true
[github]
	site = https://github.com
	endpoint = https://api.github.com
  oauth-token = SECRET
[reflow]
	git-server = GitHub
