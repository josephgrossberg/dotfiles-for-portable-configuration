[alias]
  ci = commit
  co = checkout
  di = diff
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  lo = log --oneline --abbrev-commit --abbrev=4 -25
  sb = status -sb
  st = status
[apply]
  whitespace = nowarn
[core]
  bare = false
  editor = emacs -q -l ~/git-init-file.el
  filemode = true
  ignorecase = false
  logallrefupdates = true
  pager = cat
  repositoryformatversion = 0
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[pull]
  rebase = true
[push]
  default = current
[user]
  name = Joe Grossberg
  email = josephgrossberg+github@gmail.com
